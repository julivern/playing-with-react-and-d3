{
  "name": "playing-with-react-and-d3",
  "version": "2.0.0",
  "description": "This is a demo app demonstrating how React and D3 can be friends",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test-helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "start:debug": "node --inspect-brk=5858 ./src/index.jsx",
    "start": "webpack-dev-server",
    "lint": "eslint . --ext .js --ext .jsx"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/freddyrangel/playing-with-react-and-d3.git"
  },
