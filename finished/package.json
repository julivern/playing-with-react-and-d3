    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "d3": "^3.5.16",
    "eslint": "7.30.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.0"
