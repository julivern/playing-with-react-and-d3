  },
  "homepage": "https://github.com/freddyrangel/playing-with-react-and-d3#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-react": "7.16.0",
    "babel-loader": "^8.2.3",
    "@redux-devtools/core": "^3.9.0",
    "chai": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^18.0.0",
    "mocha": "^9.1.3",
    "react": "^17.0.0",
    "react-hot-loader": "^4.13.0",
    "redux": "^4.0.0",
    "webpack": "^5.61.0",
